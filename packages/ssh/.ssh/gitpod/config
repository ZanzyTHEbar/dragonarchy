Include "gitpod/app.gitpod.io/config"

Host *.gitpod.environment
	User gitpod_devcontainer
	ProxyCommand /usr/local/bin/gitpod environment ssh %h --proxy-connect --user %r --gitpod-config-dir "$HOME/.ssh/gitpod"
	StrictHostKeyChecking no
	IdentityFile "$HOME/.ssh/gitpod/id_ed25519"
	IdentitiesOnly yes
	ControlMaster auto
